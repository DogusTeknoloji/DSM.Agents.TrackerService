<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AssemblyVersion>1.0.7257.0</AssemblyVersion>
    <FileVersion>1.0.7257.0</FileVersion>
    <Version>1.0.7234.0</Version>
    <Authors>Dogus Technology DevOps Team</Authors>
    <Company>Dogus Technology</Company>
    <Product>DSM Services Tracker Service</Product>
    <Description>DSM Site &amp; Enpoint Tracking Service</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DSM.Agents" Version="1.0.7257.3" />
    <PackageReference Include="DSM.Controller" Version="1.0.7248.5" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="TrackerService.cs">
      <SubType>Component</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
